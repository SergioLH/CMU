<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="484.0" prefWidth="630.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.vista.ListarUsuariosController">
   <top>
      <Label text="Listar Usuarios" BorderPane.alignment="CENTER">
         <font>
            <Font size="35.0" />
         </font>
         <BorderPane.margin>
            <Insets bottom="10.0" top="25.0" />
         </BorderPane.margin>
      </Label>
   </top>
   <center>
      <TableView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <columns>
            <TableColumn prefWidth="75.0" text="DNI" />
            <TableColumn prefWidth="83.0" text="Nombre" />
            <TableColumn minWidth="9.0" prefWidth="67.0" text="Apellidos" />
            <TableColumn prefWidth="75.0" text="Tipo" />
         </columns>
         <BorderPane.margin>
            <Insets bottom="15.0" left="40.0" right="40.0" />
         </BorderPane.margin>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
   <bottom>
      <GridPane BorderPane.alignment="CENTER">
         <columnConstraints>
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <JFXButton styleClass="button-raised" text="Crear Factura">
               <GridPane.margin>
                  <Insets right="15.0" />
               </GridPane.margin>
            </JFXButton>
            <JFXButton styleClass="button-raised" text="Eliminar" GridPane.columnIndex="2">
               <GridPane.margin>
                  <Insets left="15.0" />
               </GridPane.margin>
            </JFXButton>
            <JFXButton fx:id="botonEditar" styleClass="button-raised" text="Editar" GridPane.columnIndex="1" />
            <JFXButton onAction="#botonVolverMenu" styleClass="button-raised" text="Menu" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets top="15.0" />
               </GridPane.margin>
            </JFXButton>
         </children>
         <BorderPane.margin>
            <Insets bottom="35.0" left="40.0" right="40.0" />
         </BorderPane.margin>
      </GridPane>
   </bottom>
</BorderPane>
